<p id="notice"><%= notice %></p>

<h1>Givers</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Country</th>
      <th>Zipcode</th>
      <th>Description</th>
      <th>Status</th>
      <th>Wallet Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @givers.each do |giver| %>
      <tr>
        <td><%= giver.address %></td>
        <td><%= giver.latitude %></td>
        <td><%= giver.longitude %></td>
        <td><%= giver.country %></td>
        <td><%= giver.zipcode %></td>
        <td><%= giver.description %></td>
        <td><%= giver.status %></td>
        <td><%= giver.wallet_address %></td>
        <td><%= link_to 'Show', giver %></td>
        <td><%= link_to 'Edit', edit_giver_path(giver) %></td>
        <td><%= link_to 'Destroy', giver, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Giver', new_giver_path %>
